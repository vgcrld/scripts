---
# Properties to collect; no list includes all available props
:props:
  :ClusterComputeResource:
    - name
    - configIssue
    - configStatus
    - overallStatus
    - actionHistory
    - migrationHistory
    - recommendation
    - configurationEx
    - summary
  :ResourcePool:
    - name
    - configIssue
    - configStatus
    - overallStatus
    - config
    - owner
    - resourcePool
    - runtime
    - summary
  :Datastore:
    - name
    - info # Required
    - configIssue
    - configStatus
    - overallStatus
    - capability
    - summary
  :Datacenter:
    - name
    - configIssue
    - configStatus
    - overallStatus
  :HostSystem:
    - name
    - config.storageDevice.scsiTopology.adapter
    - config.network
    - hardware.systemInfo.vendor
    - hardware.systemInfo.model
    - hardware.systemInfo.uuid
    - hardware.cpuInfo
    - hardware.memorySize
    #- configIssue
    #- configStatus
    #- overallStatus
    #- capability
    #- config
    #- runtime
    #- summary
    #- systemResources
  :VirtualMachine:
    - name
    - config # Required
    - configIssue
    - configStatus
    - overallStatus
    - capability
    - guest
    - guestHeartbeatStatus
    - runtime
    - storage
    - summary
  :Network:
    - name
    - configIssue
    - configStatus
    - overallStatus
    - host
    - summary

# Valid Options:
#   - If the above lables are not part of :collect then that object is not pulled
#   - Add value - AUTO to have vcenter query the option for the object
#   - Add value - LEVEL to have vcenter query the metrics expected for that levvel
#   - Add no value by type and all metrics are queried
#   - Add a list of metrics and they are pulled, i.e: - cpu.usage.average
:collect:

  # ClusterComputeResource
  :ClusterComputeResource:
    - clusterservices.failover.latest
    - clusterservices.effectivecpu.average
    - clusterservices.effectivemem.average
    - cpu.reservedcapacity.average
    - cpu.totalmhz.average
    - cpu.usage.average
    - cpu.usagemhz.average
    - mem.active.average
    - mem.compressed.average
    - mem.compressionrate.average
    - mem.consumed.average
    - mem.decompressionrate.average
    - mem.granted.average
    - mem.overhead.average
    - mem.reservedcapacity.average
    - mem.shared.average
    - mem.swapused.average
    - mem.totalmb.average
